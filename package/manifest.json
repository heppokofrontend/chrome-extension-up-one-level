{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.0.0",
  "description": "__MSG_extDesc__",
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "worker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-scripts.js"]
    }
  ],
  "permissions": ["tabs", "storage", "contextMenus"],
  "icons": {
    "128": "icon.png"
  },
  "default_locale": "en"
}
